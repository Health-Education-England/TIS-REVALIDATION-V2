{
  "requiresCompatibilities": ["FARGATE"],
  "containerDefinitions": [
    {
      "name": "tis-revalidation-v2",
      "image": "430723991443.dkr.ecr.eu-west-2.amazonaws.com/tis-revalidation-v2/tis-revalidation-v2:1",
      "memory": 1024,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "TITLE",
          "value": "tis-revalidation-v2"
        },
        {
          "name": "AWS_REGION",
          "value": "eu-west-2"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "awslogs-reval-fe",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "reval-fe"
        }
      }
    }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "placementConstraints": [],
  "executionRoleArn": "ecsTaskExecutionRole",
  "family": "tis-revalidation-v2",
  "cpu": "256",
  "memory": "1024"
}
