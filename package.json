{
  "name": "revalidation",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:misc && ng serve",
    "start:local": "npm run build:misc && ng serve --configuration=local",
    "build": "ng build",
    "build:misc": "npm run build:shared && npm run build:browsers",
    "build:shared": "ng build --project=hee-shared",
    "build:browsers": "node supported.browsers.node.ts",
    "test": "ng test --code-coverage=true",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage=true",
    "lint": "ng lint && npm run lint:styles",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "cypress": "cypress run",
    "cypress-open": "cypress open",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "preinstall": "npx npm-force-resolutions"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@angular/service-worker": "~9.1.9",
    "@ngxs/logger-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "angulartics2": "^9.1.0",
    "aws-amplify": "^3.0.17",
    "nhsuk-frontend": "^3.1.0",
    "rxjs": "~6.5.5",
    "tslib": "^1.13.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/language-service": "~9.1.9",
    "@aws-amplify/pubsub": "^2.1.9",
    "@ngxs/cli": "^2.0.0",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@react-native-community/netinfo": "^5.9.2",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.12",
    "browserslist-useragent-regexp": "^2.0.5",
    "buffer": "^5.6.0",
    "codelyzer": "^5.2.2",
    "cypress": "^4.8.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^5.0.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^9.1.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rollup": "^2.15.0",
    "stylelint": "^13.6.0",
    "stylelint-prettier": "^1.1.2",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.8.3"
  },
